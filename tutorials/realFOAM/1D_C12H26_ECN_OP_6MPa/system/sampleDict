FoamFile
{
        version         2.0;
        format          ascii;
        class           dictionary;
        location        system;
        object          sampleDict;
}

type sets;
libs ("libsampling.so");

interpolationScheme cellPoint;

setFormat raw;

fields          ( T rho);

sets
(
line_0
{
         type        midPoint;
         axis        x;
         start       (0 0 0);
         end         (0.1 0 0);
}
);
